<?xml version="1.0" encoding="UTF-8"?>

<faces-config
	xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
version="1.2">
    <component>
        <component-type>PublicEventCreator</component-type>
        <component-class>com.idega.block.caldav.presentation.PublicEventCreator</component-class>
        <component-extension>
            <idegaweb-info>
                <builder-visible>true</builder-visible>
                <object-type>jsf.uicomponent</object-type>
            </idegaweb-info>
        </component-extension>
    </component>
    <component>
        <component-type>PrivateEventCreator</component-type>
        <component-class>com.idega.block.caldav.presentation.PrivateEventCreator</component-class>
        <component-extension>
            <idegaweb-info>
                <builder-visible>true</builder-visible>
                <object-type>jsf.uicomponent</object-type>
            </idegaweb-info>
        </component-extension>
    </component>
    <component>
        <component-type>EventViewer</component-type>
        <component-class>com.idega.block.caldav.presentation.EventViewer</component-class>
        <component-extension>
            <idegaweb-info>
                <builder-visible>true</builder-visible>
                <object-type>jsf.uicomponent</object-type>
            </idegaweb-info>
        </component-extension>
    </component>
</faces-config>
